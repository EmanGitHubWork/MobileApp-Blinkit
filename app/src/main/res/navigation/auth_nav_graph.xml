<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.blinkit.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_splashFragment_to_signInFragment2"
            app:destination="@id/signInFragment"
            app:popUpTo="@id/splashFragment" />
    </fragment>

    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.blinkit.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_OTP_Fragment"
            app:destination="@id/OTP_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/OTP_Fragment"
        android:name="com.example.blinkit.OTP_Fragment"
        android:label="fragment_o_t_p_"
        tools:layout="@layout/fragment_o_t_p_" >
        <action
            android:id="@+id/action_OTP_Fragment_to_newHome_Fragment"
            app:destination="@id/newHome_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/newHome_Fragment"
        android:name="com.example.blinkit.NewHomeFragment"
        android:label="fragment_new_home_"
        tools:layout="@layout/fragment_new_home_" >
        <action
            android:id="@+id/action_newHome_Fragment_to_orderAgainFragment"
            app:destination="@id/orderAgainFragment" />
        <action
            android:id="@+id/action_newHome_Fragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_newHome_Fragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
        <action
            android:id="@+id/action_newHome_Fragment_to_categoryProductFragment"
            app:destination="@id/categoryProductFragment" />
        <action
            android:id="@+id/action_newHome_Fragment_to_cartFragment"
            app:destination="@id/cartFragment" />

    </fragment>
    <fragment
        android:id="@+id/orderAgainFragment"
        android:name="com.example.blinkit.OrderAgainFragment"
        android:label="fragment_order_again"
        tools:layout="@layout/fragment_order_again" >
        <action
            android:id="@+id/action_orderAgainFragment_to_cartFragment"
            app:destination="@id/cartFragment" />

    </fragment>
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.example.blinkit.CategoriesFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_categoriesFragment_to_cartFragment"
            app:destination="@id/cartFragment" />

    </fragment>

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.blinkit.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_profileFragment_to_otpFragment"
            app:destination="@id/OTP_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryProductFragment"
        android:name="com.example.blinkit.CategoryProductFragment"
        android:label="fragment_category_product"
        tools:layout="@layout/fragment_category_product" >
        <action
            android:id="@+id/action_categoryProductFragment_to_cartFragment"
            app:destination="@id/cartFragment" />

    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.blinkit.CartFragment"
        android:label="CartFragment">
        <action
            android:id="@+id/action_cartFragment_to_orderplaceFragment"
            app:destination="@id/orderplaceFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderplaceFragment"
        android:name="com.example.blinkit.OrderplaceFragment"
        android:label="OrderplaceFragment"
       >
        <action
            android:id="@+id/action_orderplaceFragment_to_newHome_Fragment"
            app:destination="@id/newHome_Fragment" />

    </fragment>

</navigation>